{
  "name": "nci-ansible-ui-client",
  "version": "0.1.0-alpha.1",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "bootstrap": "^3.4.1",
    "data.io": "^0.3.1",
    "font-awesome": "^4.7.0",
    "less": "^3.12.2",
    "mobx": "^6.0.4",
    "mobx-react": "^7.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "scmicons": "^1.3.0",
    "socket.io-client": "^2.3.1",
    "underscore": "^1.11.0",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "npm run buildLegacyStyles && PORT=4000 react-scripts start",
    "dev": "npm start",
    "_buildLegacyFonts": "mkdir -p src/legacyStyles/dist/fonts && cp ./node_modules/bootstrap/fonts/* ./src/legacyStyles/dist/fonts/ && cp ./node_modules/font-awesome/fonts/* ./src/legacyStyles/dist/fonts/ && cp ./node_modules/scmicons/fonts/* ./src/legacyStyles/dist/fonts/",
    "buildLegacyStyles": "mkdir -p src/legacyStyles/dist/css && npm run _buildLegacyFonts && lessc src/legacyStyles/index.less > src/legacyStyles/dist/css/index.css",
    "build": "npm run buildLegacyStyles && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://127.0.0.1:3000"
}
